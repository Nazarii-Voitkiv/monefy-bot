# Copy this file to .env and fill values before running with Docker
# Bot
BOT_TOKEN=your_telegram_bot_token_here

# Database - for docker compose the host will be 'db'
POSTGRES_USER=postgres
POSTGRES_PASSWORD=change_me
POSTGRES_DB=postgres
# DATABASE_URL can be left empty and will be constructed from POSTGRES_* vars
DATABASE_URL=postgresql://postgres:change_me@db:5432/postgres

MODE=polling
PORT=3000
